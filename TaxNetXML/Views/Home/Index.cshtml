@*@model IEnumerable<TaxNetXML.Models.User>*@
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h3>Users</h3>
    <table>
        <tr class="header">
            <td><p>id</p></td>
            <td><p>name</p></td>
            <td><p>age</p></td>
            @*<td></td>*@
        </tr>
        @foreach (var b in ViewBag.Users) {
            <tr>
                <td><p>@b.Id</p></td>
                <td><p>@b.Name</p></td>
                <td><p>@b.Age</p></td>
                @*<td><p><a href="/Home/Buy/@b.Id">Купить</a></p></td>*@
            </tr>}
    </table>
</div>